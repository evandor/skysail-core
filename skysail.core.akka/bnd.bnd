Bundle-Name: SKYSAIL :: core :: akka

Bundle-Version: 0.1.0.${tstamp}

Import-Package: \
    io.skysail.api.weaving,\
    *
    
-includeresource: OSGI-OPT/src=src/
    
#Require-Capability: io.skysail.api.um

-buildpath: \
	${osgi},\
	${logging},\
	org.scalatest;version=3.0,\
	org.scalactic;version=3.0,\
	org.scala-lang.modules.scala-xml;version=1.0,\
	org.assertj.core;version=3.5,\
	io.spray.json;version=1.3,\
	com.typesafe.akka.osgi;version=${akka.version},\
	com.typesafe.akka.actor;version=${akka.version},\
	com.typesafe.akka.stream;version=${akka.version},\
	com.typesafe.akka.http.core;version=${akka.version},\
	com.typesafe.akka.http.experimental;version=${akka.version},\
	com.typesafe.akka.http.spray.json.experimental;version=${akka.version},\
	com.typesafe.config;version=1.3,\
	org.reactivestreams.reactive-streams;version=1.0,\
    org.scala-lang.modules.scala-java8-compat;version=0.8,\
	com.typesafe.akka.testkit;version=${akka.version}

Bundle-Activator: io.skysail.core.akka.Activator

Private-Package: io.skysail.core.akka